{
  "name": "woll",
  "version": "1.0.0",
  "description": "woll聊天工具",
  "author": "黄川",
  "scripts": {
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue src",
    "build:prod": "vue-cli-service build src/main-web.js",
    "build:prod-report": "vue-cli-service build src/main-web.js --report",
    "build:stage": "vue-cli-service build --mode staging",
    "dev": "vue-cli-service serve src/main.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve src/main-client.js",
    "new": "plop",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "main": "background.js",
  "dependencies": {
    "@riophae/vue-treeselect": "0.4.0",
